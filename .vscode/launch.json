            },
            "args": [
                "scan",
                "--input", 
                "E:/HOME/RetroVault/TOSEC_DATs/Extracted/TOSEC-v2025-03-13",
                "--output",
                "E:/HOME/Documents/Databases/turbo-tosec/duckdb/tosec-v2025-03-13.duckdb",
                "--staged", 
                "--workers", 
                "4",
                "--temp-dir",
                "debug_temp_chunks"
            ],
            "console": "integratedTerminal",
